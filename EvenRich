package com.dsa.array;

public class EvenRich {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
      int []arr= {1,3,4,5,7,9,8,7,58,98};
      int even=0;
      for(int i:arr) {
    	  if(i%2==0) 
    		 // System.out.println(" "+i);
    		  even=even+1;  
      }
	
     System.out.println(" "+even);
      
	}

}
