package com.dsa.array;

public class Missingnumbe {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
       int []arr= {1,2,4,5,6,7,8,9};
       int n=9;
       int total=n*(n+1)/2;
       int sum=0;
       for(int i:arr) {
    	   sum+=i;
    	   //int mising=total-sum;
       }
	   System.out.println("Mising ="+(total-sum));

	}

}
